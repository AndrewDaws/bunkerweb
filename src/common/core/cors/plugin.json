{
  "id": "cors",
  "name": {
    "en": "CORS",
    "fr": "CORS"
  },
  "description": {
    "en": "Cross-Origin Resource Sharing.",
    "fr": "Cross-Origin Resource Sharing."
  },
  "version": "1.0",
  "stream": "no",
  "settings": {
    "USE_CORS": {
      "context": "multisite",
      "default": "no",
      "help": {
        "en": "Use CORS",
        "fr": "Utiliser CORS"
      },
      "id": "use-cors",
      "label": {
        "en": "Use CORS",
        "fr": "Utiliser CORS"
      },
      "regex": "^(yes|no)$",
      "type": "check"
    },
    "CORS_ALLOW_ORIGIN": {
      "context": "multisite",
      "default": "*",
      "help": {
        "en": "Allowed origins to make CORS requests : PCRE regex or *.",
        "fr": "Origines autorisées à faire des requêtes CORS : PCRE regex ou *."
      },
      "id": "cors-allow-origin",
      "label": {
        "en": "Allowed origins",
        "fr": "Origines autorisées"
      },
      "regex": "^.*$",
      "type": "text"
    },
    "CORS_EXPOSE_HEADERS": {
      "context": "multisite",
      "default": "Content-Length,Content-Range",
      "help": {
        "en": "Value of the Access-Control-Expose-Headers header.",
        "fr": "Valeur de l'en-tête Access-Control-Expose-Headers."
      },
      "id": "cors-expose-headers",
      "label": {
        "en": "Access-Control-Expose-Headers value",
        "fr": "Valeur de l'en-tête Access-Control-Expose-Headers"
      },
      "regex": "^(\\*|(?![, ]+)(,? ?([\\w\\-]+)(?!.*\\3(?!.)))*)?$",
      "type": "text"
    },
    "CROSS_ORIGIN_OPENER_POLICY": {
      "context": "multisite",
      "default": "",
      "help": {
        "en": "Value for the Cross-Origin-Opener-Policy header.",
        "fr": "Valeur de l'en-tête Cross-Origin-Opener-Policy."
      },
      "id": "cross-origin-opener-policy",
      "label": {
        "en": "Cross-Origin-Opener-Policy",
        "fr": "Cross-Origin-Opener-Policy"
      },
      "regex": "^(unsafe-none|same-origin-allow-popups|same-origin)?$",
      "type": "select",
      "select": ["", "unsafe-none", "same-origin-allow-popups", "same-origin"]
    },
    "CROSS_ORIGIN_EMBEDDER_POLICY": {
      "context": "multisite",
      "default": "",
      "help": {
        "en": "Value for the Cross-Origin-Embedder-Policy header.",
        "fr": "Valeur de l'en-tête Cross-Origin-Embedder-Policy."
      },
      "id": "cross-origin-embedder-policy",
      "label": {
        "en": "Cross-Origin-Embedder-Policy",
        "fr": "Cross-Origin-Embedder-Policy"
      },
      "regex": "^(unsafe-none|require-corp|credentialless)?$",
      "type": "select",
      "select": ["", "unsafe-none", "require-corp", "credentialless"]
    },
    "CROSS_ORIGIN_RESOURCE_POLICY": {
      "context": "multisite",
      "default": "",
      "help": {
        "en": "Value for the Cross-Origin-Resource-Policy header.",
        "fr": "Valeur de l'en-tête Cross-Origin-Resource-Policy."
      },
      "id": "cross-origin-resource-policy",
      "label": {
        "en": "Cross-Origin-Resource-Policy",
        "fr": "Cross-Origin-Resource-Policy"
      },
      "regex": "^(same-site|same-origin|cross-origin)?$",
      "type": "select",
      "select": ["", "same-site", "same-origin", "cross-origin"]
    },
    "CORS_MAX_AGE": {
      "context": "multisite",
      "default": "86400",
      "help": {
        "en": "Value of the Access-Control-Max-Age header.",
        "fr": "Valeur de l'en-tête Access-Control-Max-Age."
      },
      "id": "cors-max-age",
      "label": {
        "en": "Access-Control-Max-Age value",
        "fr": "Valeur de l'en-tête Access-Control-Max-Age"
      },
      "regex": "^\\d+$",
      "type": "text"
    },
    "CORS_ALLOW_CREDENTIALS": {
      "context": "multisite",
      "default": "no",
      "help": {
        "en": "Send the Access-Control-Allow-Credentials header.",
        "fr": "Envoyer l'en-tête Access-Control-Allow-Credentials."
      },
      "id": "cors-allow-credentials",
      "label": {
        "en": "Send Access-Control-Allow-Credentials",
        "fr": "Envoyer Access-Control-Allow-Credentials"
      },
      "regex": "^(yes|no)$",
      "type": "check"
    },
    "CORS_ALLOW_METHODS": {
      "context": "multisite",
      "default": "GET, POST, OPTIONS",
      "help": {
        "en": "Value of the Access-Control-Allow-Methods header.",
        "fr": "Valeur de l'en-tête Access-Control-Allow-Methods."
      },
      "id": "cors-allow-methods",
      "label": {
        "en": "Access-Control-Allow-Methods value",
        "fr": "Valeur de l'en-tête Access-Control-Allow-Methods"
      },
      "regex": "^(\\*|(?![, ])(,? ?(GET|HEAD|POST|PUT|DELETE|CONNECT|OPTIONS|TRACE|PATCH)(?!.*\\3))*)?$",
      "type": "text"
    },
    "CORS_ALLOW_HEADERS": {
      "context": "multisite",
      "default": "DNT,User-Agent,X-Requested-With,If-Modified-Since,Cache-Control,Content-Type,Range",
      "help": {
        "en": "Value of the Access-Control-Allow-Headers header.",
        "fr": "Valeur de l'en-tête Access-Control-Allow-Headers."
      },
      "id": "cors-allow-headers",
      "label": {
        "en": "Access-Control-Allow-Headers value",
        "fr": "Valeur de l'en-tête Access-Control-Allow-Headers"
      },
      "regex": "^(\\*|(?![, ])(,? ?([\\w\\-]+)(?!.*\\3(?!.)))*)?$",
      "type": "text"
    },
    "CORS_DENY_REQUEST": {
      "context": "multisite",
      "default": "yes",
      "help": {
        "en": "Deny request and don't send it to backend if Origin is not allowed.",
        "fr": "Refuser la requête et ne pas l'envoyer au backend si l'origine n'est pas autorisée."
      },
      "id": "cors-deny-request",
      "label": {
        "en": "Deny request",
        "fr": "Refuser la requête"
      },
      "regex": "^(yes|no)$",
      "type": "check"
    }
  }
}
