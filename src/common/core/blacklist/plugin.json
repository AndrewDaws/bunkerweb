{
  "id": "blacklist",
  "name": {
    "en": "Blacklist",
    "fr": "Liste noire"
  },
  "description": {
    "en": "Deny access based on internal and external IP/network/rDNS/ASN blacklists.",
    "fr": "Refuser l'accès en fonction des listes noires IP/rDNS/ASN internes et externes."
  },
  "version": "1.0",
  "stream": "partial",
  "settings": {
    "USE_BLACKLIST": {
      "context": "multisite",
      "default": "yes",
      "help": {
        "en": "Activate blacklist feature.",
        "fr": "Activer la fonctionnalité de liste noire."
      },
      "id": "use-blacklist",
      "label": {
        "en": "Activate blacklist",
        "fr": "Activer la liste noire"
      },
      "regex": "^(yes|no)$",
      "type": "check"
    },
    "BLACKLIST_IP": {
      "context": "multisite",
      "default": "",
      "help": {
        "en": "List of IP/network, separated with spaces, to block.",
        "fr": "Liste d'IP/réseau, séparés par des espaces, à bloquer."
      },
      "id": "blacklist-ip",
      "label": {
        "en": "Blacklist IP/network",
        "fr": "Liste noire IP/réseau"
      },
      "regex": "^(?! )( *(((\\b25[0-5]|\\b2[0-4]\\d|\\b[01]?\\d\\d?)(\\.(25[0-5]|2[0-4]\\d|[01]?\\d\\d?)){3})(\\/([1-2][0-9]?|3[0-2]?|[04-9]))?|(([0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:)|fe80:(:[0-9a-fA-F]Z{0,4}){0,4}%[0-9a-zA-Z]+|::(ffff(:0{1,4})?:)?((25[0-5]|(2[0-4]|1?\\d)?\\d)\\.){3}(25[0-5]|(2[0-4]|1?\\d)?\\d)|([0-9a-fA-F]{1,4}:){1,4}:((25[0-5]|(2[0-4]|1?\\d)?\\d)\\.){3}(25[0-5]|(2[0-4]|1?\\d)?\\d))(\\/(12[0-8]|1[01][0-9]|[0-9][0-9]?))?)(?!.*\\D\\2([^\\d\\/]|$)) *)*$",
      "type": "text"
    },
    "BLACKLIST_IP_URLS": {
      "context": "global",
      "default": "https://www.dan.me.uk/torlist/?exit",
      "help": {
        "en": "List of URLs, separated with spaces, containing bad IP/network to block.",
        "fr": "Liste d'URLs, séparés par des espaces, contenant des IP/réseau à bloquer."
      },
      "id": "blacklist-ip-urls",
      "label": {
        "en": "Blacklist IP/network URLs",
        "fr": "Liste noire IP/réseau URLs"
      },
      "regex": "^( *((https?:\\/\\/|file:\\/\\/\\/)[\\-\\w@:%.+~#=]+[\\-\\w\\(\\)!@:%+.~#?&\\/=$]*)(?!.*\\2(?!.)) *)*$",
      "type": "text"
    },
    "BLACKLIST_RDNS_GLOBAL": {
      "context": "multisite",
      "default": "yes",
      "help": {
        "en": "Only perform RDNS blacklist checks on global IP addresses.",
        "fr": "Ne vérifier les listes noires DNS inverses que sur les adresses IP globales."
      },
      "id": "blacklist-rdns-global",
      "label": {
        "en": "Blacklist reverse DNS global IPs",
        "fr": "Liste noire DNS inverses adresses IP globales"
      },
      "regex": "^(yes|no)$",
      "type": "check"
    },
    "BLACKLIST_RDNS": {
      "context": "multisite",
      "default": ".shodan.io .censys.io",
      "help": {
        "en": "List of reverse DNS suffixes, separated with spaces, to block.",
        "fr": "Liste de suffixes DNS inverses, séparés par des espaces, à bloquer."
      },
      "id": "blacklist-rdns",
      "label": {
        "en": "Blacklist reverse DNS",
        "fr": "Liste noire DNS inverses"
      },
      "regex": "^( *(([^ ]+)(?!.*\\3( |$))) *)*$",
      "type": "text"
    },
    "BLACKLIST_RDNS_URLS": {
      "context": "global",
      "default": "",
      "help": {
        "en": "List of URLs, separated with spaces, containing reverse DNS suffixes to block.",
        "fr": "Liste d'URLs, séparés par des espaces, contenant des suffixes DNS inverses à bloquer."
      },
      "id": "blacklist-rdns-urls",
      "label": {
        "en": "Blacklist reverse DNS URLs",
        "fr": "Liste noire DNS inverses URLs"
      },
      "regex": "^( *((https?:\\/\\/|file:\\/\\/\\/)[\\-\\w@:%.+~#=]+[\\-\\w\\(\\)!@:%+.~#?&\\/=$]*)(?!.*\\2(?!.)) *)*$",
      "type": "text"
    },
    "BLACKLIST_ASN": {
      "context": "multisite",
      "default": "",
      "help": {
        "en": "List of ASN numbers, separated with spaces, to block.",
        "fr": "Liste de numéros ASN, séparés par des espaces, à bloquer."
      },
      "id": "blacklist-asn",
      "label": {
        "en": "Blacklist ASN",
        "fr": "Liste noire ASN"
      },
      "regex": "^^( *((ASN?)?(\\d+)\\b(?!.*[SN ]\\4\\b)) *)*$",
      "type": "text"
    },
    "BLACKLIST_ASN_URLS": {
      "context": "global",
      "default": "",
      "help": {
        "en": "List of URLs, separated with spaces, containing ASN to block.",
        "fr": "Liste d'URLs, séparés par des espaces, contenant des ASN à bloquer."
      },
      "id": "blacklist-asn-urls",
      "label": {
        "en": "Blacklist ASN URLs",
        "fr": "Liste noire ASN URLs"
      },
      "regex": "^( *((https?:\\/\\/|file:\\/\\/\\/)[\\-\\w@:%.+~#=]+[\\-\\w\\(\\)!@:%+.~#?&\\/=$]*)(?!.*\\2(?!.)) *)*$",
      "type": "text"
    },
    "BLACKLIST_USER_AGENT": {
      "context": "multisite",
      "default": "",
      "help": {
        "en": "List of User-Agent (PCRE regex), separated with spaces, to block.",
        "fr": "Liste de User-Agent (regex PCRE), séparés par des espaces, à bloquer."
      },
      "id": "blacklist-user-agent",
      "label": {
        "en": "Blacklist User-Agent",
        "fr": "Liste noire User-Agent"
      },
      "regex": "^.*$",
      "type": "text"
    },
    "BLACKLIST_USER_AGENT_URLS": {
      "context": "global",
      "default": "https://raw.githubusercontent.com/mitchellkrogza/nginx-ultimate-bad-bot-blocker/master/_generator_lists/bad-user-agents.list",
