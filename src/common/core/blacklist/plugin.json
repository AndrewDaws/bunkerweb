      "regex": "^( *(.*)(?!.*\\2(?!.)) *)*$",
      "type": "text"
    },
    "BLACKLIST_URI_URLS": {
      "context": "global",
      "default": "",
      "help": {
        "en": "List of URLs, separated with spaces, containing bad URI to block.",
        "fr": "Liste d'URLs, séparés par des espaces, contenant des URI à bloquer."
