{
  "id": "ui",
  "name": {
    "en": "UI",
    "fr": "Interface utilisateur"
  },
  "description": {
    "en": "Integrate easily the BunkerWeb UI.",
    "fr": "Int√©grez facilement l'interface utilisateur de BunkerWeb."
  },
  "version": "1.0",
  "stream": "no",
  "settings": {
    "USE_UI": {
      "context": "multisite",
      "default": "no",
      "help": {
        "en": "Use UI",
        "fr": "Utiliser l'interface utilisateur"
      },
      "id": "use-ui",
      "label": {
        "en": "Use UI",
        "fr": "Utiliser l'interface utilisateur"
      },
      "regex": "^(yes|no)$",
      "type": "check"
    },
    "UI_HOST": {
      "context": "global",
      "default": "",
      "help": "Address of the web UI used for initial setup",
      "id": "ui-host",
      "label": "UI host",
      "regex": "^.*$",
      "type": "text"
    }
  }
}
