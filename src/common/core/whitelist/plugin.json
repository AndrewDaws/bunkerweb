{
  "id": "whitelist",
  "name": {
    "en": "Whitelist",
    "fr": "Liste blanche"
  },
  "description": {
    "en": "Allow access based on internal and external IP/network/rDNS/ASN whitelists.",
    "fr": "Autoriser l'accès en fonction des listes blanches d'IP/réseau/rDNS/ASN internes et externes."
  },
  "version": "1.0",
  "stream": "partial",
  "settings": {
    "USE_WHITELIST": {
      "context": "multisite",
      "default": "yes",
      "help": {
        "en": "Activate whitelist feature.",
        "fr": "Activer la fonctionnalité de liste blanche."
      },
      "id": "use-whitelist",
      "label": {
        "en": "Use whitelist",
        "fr": "Utiliser la liste blanche"
      },
      "regex": "^(yes|no)$",
      "type": "check"
    },
    "WHITELIST_IP": {
      "context": "multisite",
      "default": "20.191.45.212 40.88.21.235 40.76.173.151 40.76.163.7 20.185.79.47 52.142.26.175 20.185.79.15 52.142.24.149 40.76.162.208 40.76.163.23 40.76.162.191 40.76.162.247",
      "help": {
        "en": "List of IP/network, separated with spaces, to put into the whitelist.",
        "fr": "Liste d'IP/réseau, séparés par des espaces, à mettre dans la liste blanche."
      },
      "id": "whitelist-ip",
      "label": {
        "en": "Whitelist IP/network",
        "fr": "Liste blanche IP/réseau"
      },
      "regex": "^(?! )( *(((\\b25[0-5]|\\b2[0-4]\\d|\\b[01]?\\d\\d?)(\\.(25[0-5]|2[0-4]\\d|[01]?\\d\\d?)){3})(\\/([1-2][0-9]?|3[0-2]?|[04-9]))?|(([0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:)|fe80:(:[0-9a-fA-F]Z{0,4}){0,4}%[0-9a-zA-Z]+|::(ffff(:0{1,4})?:)?((25[0-5]|(2[0-4]|1?\\d)?\\d)\\.){3}(25[0-5]|(2[0-4]|1?\\d)?\\d)|([0-9a-fA-F]{1,4}:){1,4}:((25[0-5]|(2[0-4]|1?\\d)?\\d)\\.){3}(25[0-5]|(2[0-4]|1?\\d)?\\d))(\\/(12[0-8]|1[01][0-9]|[0-9][0-9]?))?)(?!.*\\D\\2([^\\d\\/]|$)) *)*$",
      "type": "text"
    },
    "WHITELIST_IP_URLS": {
      "context": "global",
      "default": "",
      "help": {
        "en": "List of URLs, separated with spaces, containing good IP/network to whitelist.",
        "fr": "Liste d'URLs, séparés par des espaces, contenant des IP/réseau à mettre dans la liste blanche."
      },
      "id": "whitelist-ip-urls",
      "label": {
        "en": "Whitelist IP/network URLs",
        "fr": "Liste blanche URLs IP/réseau"
      },
      "regex": "^( *((https?:\\/\\/|file:\\/\\/\\/)[\\-\\w@:%.+~#=]+[\\-\\w\\(\\)!@:%+.~#?&\\/=$]*)(?!.*\\2(?!.)) *)*$",
      "type": "text"
    },
    "WHITELIST_RDNS_GLOBAL": {
      "context": "multisite",
      "default": "yes",
      "help": {
        "en": "Only perform RDNS whitelist checks on global IP addresses.",
        "fr": "Ne vérifier les listes blanches de reverse DNS que sur les adresses IP globales."
      },
      "id": "whitelist-rdns-global",
      "label": {
        "en": "Whitelist reverse DNS global IPs",
        "fr": "Liste blanche reverse DNS IP globales"
      },
      "regex": "^(yes|no)$",
      "type": "check"
    },
    "WHITELIST_RDNS": {
      "context": "multisite",
      "default": ".google.com .googlebot.com .yandex.ru .yandex.net .yandex.com .search.msn.com .baidu.com .baidu.jp .crawl.yahoo.net .fwd.linkedin.com .twitter.com .twttr.com .discord.com",
      "help": {
        "en": "List of reverse DNS suffixes, separated with spaces, to whitelist.",
        "fr": "Liste de suffixes de reverse DNS, séparés par des espaces, à mettre dans la liste blanche."
      },
      "id": "whitelist-rdns",
      "label": {
        "en": "Whitelist reverse DNS",
        "fr": "Liste blanche reverse DNS"
      },
      "regex": "^( *(([^ ]+)(?!.*\\3( |$))) *)*$",
      "type": "text"
    },
    "WHITELIST_RDNS_URLS": {
      "context": "global",
      "default": "",
      "help": {
        "en": "List of URLs, separated with spaces, containing reverse DNS suffixes to whitelist.",
        "fr": "Liste d'URLs, séparés par des espaces, contenant des suffixes de reverse DNS à mettre dans la liste blanche."
      },
      "id": "whitelist-rdns-urls",
      "label": {
        "en": "Whitelist reverse DNS URLs",
        "fr": "Liste blanche reverse DNS URLs"
      },
      "regex": "^( *((https?:\\/\\/|file:\\/\\/\\/)[\\-\\w@:%.+~#=]+[\\-\\w\\(\\)!@:%+.~#?&\\/=$]*)(?!.*\\2(?!.)) *)*$",
      "type": "text"
    },
    "WHITELIST_ASN": {
      "context": "multisite",
      "default": "32934",
      "help": {
        "en": "List of ASN numbers, separated with spaces, to whitelist.",
        "fr": "Liste de numéros ASN, séparés par des espaces, à mettre dans la liste blanche."
      },
      "id": "whitelist-asn",
      "label": {
        "en": "Whitelist ASN",
        "fr": "Liste blanche ASN"
      },
      "regex": "^^( *((ASN?)?(\\d+)\\b(?!.*[SN ]\\4\\b)) *)*$",
      "type": "text"
    },
    "WHITELIST_ASN_URLS": {
      "context": "global",
      "default": "",
      "help": {
        "en": "List of URLs, separated with spaces, containing ASN to whitelist.",
        "fr": "Liste d'URLs, séparés par des espaces, contenant des ASN à mettre dans la liste blanche."
      },
      "id": "whitelist-asn-urls",
      "label": {
        "en": "Whitelist ASN URLs",
        "fr": "Liste blanche ASN URLs"
      },
      "regex": "^( *((https?:\\/\\/|file:\\/\\/\\/)[\\-\\w@:%.+~#=]+[\\-\\w\\(\\)!@:%+.~#?&\\/=$]*)(?!.*\\2(?!.)) *)*$",
      "type": "text"
    },
    "WHITELIST_USER_AGENT": {
      "context": "multisite",
      "default": "",
      "help": {
        "en": "List of User-Agent (PCRE regex), separated with spaces, to whitelist.",
        "fr": "Liste de User-Agent (regex PCRE), séparés par des espaces, à mettre dans la liste blanche."
      },
      "id": "whitelist-user-agent",
      "label": {
        "en": "Whitelist User-Agent",
        "fr": "Liste blanche User-Agent"
      },
      "regex": "^.*$",
      "type": "text"
    },
    "WHITELIST_USER_AGENT_URLS": {
      "context": "global",
      "default": "",
      "help": {
        "en": "List of URLs, separated with spaces, containing bad User-Agent to whitelist.",
        "fr": "Liste d'URLs, séparés par des espaces, contenant des User-Agent à mettre dans la liste blanche."
      },
      "id": "whitelist-user-agent-urls",
      "label": {
        "en": "Whitelist User-Agent URLs",
        "fr": "Liste blanche User-Agent URLs"
      },
      "regex": "^( *((https?:\\/\\/|file:\\/\\/\\/)[\\-\\w@:%.+~#=]+[\\-\\w\\(\\)!@:%+.~#?&\\/=$]*)(?!.*\\2(?!.)) *)*$",
      "type": "text"
    },
    "WHITELIST_URI": {
      "context": "multisite",
      "default": "",
      "help": {
        "en": "List of URI (PCRE regex), separated with spaces, to whitelist.",
        "fr": "Liste de URI (regex PCRE), séparés par des espaces, à mettre dans la liste blanche."
      },
      "id": "whitelist-uri",
      "label": {
        "en": "Whitelist URI",
        "fr": "Liste blanche URI"
      },
      "regex": "^( *(.*)(?!.*\\2(?!.)) *)*$",
      "type": "text"
    },
    "WHITELIST_URI_URLS": {
      "context": "global",
      "default": "",
      "help": {
        "en": "List of URLs, separated with spaces, containing bad URI to whitelist.",
        "fr": "Liste d'URLs, séparés par des espaces, contenant des URI à mettre dans la liste blanche."
      },
      "id": "whitelist-uri-urls",
      "label": {
        "en": "Whitelist URI URLs",
        "fr": "Liste blanche URI URLs"
      },
      "regex": "^( *((https?:\\/\\/|file:\\/\\/\\/)[\\-\\w@:%.+~#=]+[\\-\\w\\(\\)!@:%+.~#?&\\/=$]*)(?!.*\\2(?!.)) *)*$",
      "type": "text"
    }
  },
  "jobs": [
    {
      "name": "whitelist-download",
      "file": "whitelist-download.py",
      "every": "hour",
      "reload": true
    }
  ]
}
